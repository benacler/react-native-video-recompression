{
    "name": "react-native-video-recompression",
    "version": "0.9.4",
    "description": "A React Native library for intelligent video processing with native performance. Supports video analysis, quality-preserving format conversion, and smart recompression with customizable settings.",
    "main": "lib/commonjs/index.js",
    "module": "lib/module/index.js",
    "types": "lib/typescript/index.d.ts",
    "react-native": "src/index.ts",
    "source": "src/index.ts",
    "files": [
        "src",
        "lib",
        "android",
        "ios",
        "react-native-video-recompression.podspec",
        "!android/build",
        "!android/.cxx",
        "!android/local.properties",
        "!**/__tests__",
        "!**/__fixtures__",
        "!**/__mocks__",
        "!lib/typescript/example",
        "!android/src/main/java/com/videorecompression/VideoRecompressionPackage.spec.ts"
    ],
    "scripts": {
        "test": "jest",
        "typecheck": "tsc --noEmit",
        "lint": "eslint \"**/*.{js,ts,tsx}\"",
        "lint:fix": "eslint \"**/*.{js,ts,tsx}\" --fix",
        "format": "prettier --write \"**/*.{js,ts,tsx,json,md}\"",
        "prepare": "npm run build",
        "build": "npm run build:js && npm run build:types",
        "build:js": "npm run build:commonjs && npm run build:module",
        "build:commonjs": "tsc --project tsconfig.build.json --module commonjs --outDir lib/commonjs",
        "build:module": "tsc --project tsconfig.build.json --module esnext --outDir lib/module",
        "build:types": "tsc --project tsconfig.build.json --declaration --emitDeclarationOnly --outDir lib/typescript",
        "clean": "rimraf lib",
        "create-test-videos": "./scripts/create-test-videos.sh",
        "validate": "./scripts/validate-library.sh",
        "pre-publish": "./scripts/pre-publish.sh",
        "release": "semantic-release"
    },
    "keywords": [
        "react-native",
        "video",
        "compression",
        "recompression",
        "video-processing",
        "video-analysis",
        "format-conversion",
        "video-formats",
        "ios",
        "android",
        "codec",
        "ffmpeg-alternative",
        "native-performance",
        "video-metadata",
        "video-transcoding",
        "mobile"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/benacler/react-native-video-recompression.git"
    },
    "author": "Leo <leo@example.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/benacler/react-native-video-recompression/issues"
    },
    "homepage": "https://github.com/benacler/react-native-video-recompression#readme",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    },
    "devDependencies": {
        "@babel/preset-typescript": "^7.27.1",
        "@commitlint/config-conventional": "^18.4.3",
        "@react-native-community/eslint-config": "^3.2.0",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/commit-analyzer": "^11.1.0",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^9.2.4",
        "@semantic-release/npm": "^11.0.2",
        "@semantic-release/release-notes-generator": "^12.1.0",
        "@types/jest": "^29.5.8",
        "@types/react": "^18.2.38",
        "@types/react-native": "0.72.7",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "commitlint": "^18.4.3",
        "eslint": "^8.54.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.1",
        "husky": "^8.0.3",
        "jest": "^29.7.0",
        "lint-staged": "^15.1.0",
        "pod-install": "^0.1.39",
        "prettier": "^3.1.0",
        "react": "18.2.0",
        "react-native": "0.72.7",
        "rimraf": "^5.0.5",
        "semantic-release": "^22.0.8",
        "typescript": "^5.3.2"
    },
    "peerDependencies": {
        "react": "*",
        "react-native": "*"
    },
    "engines": {
        "node": ">= 18.0.0",
        "npm": ">= 8.0.0"
    }
}